{
    "plugins": [
        [
            "module-resolver",
            {
                "root": ["."],
                "alias":{
                    "styles": "scss/"
                },
                "crw": "babelrc"
            }
        ],
        [
            "wrap-in-js",
            {
                "extensions": ["css$", "scss$"]
            }
        ]
    ],
    "presets": ["next/babel"],
    "ignore": []
}